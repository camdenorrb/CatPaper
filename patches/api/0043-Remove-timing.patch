-        if (event.isAsynchronous() || !Timings.timingsEnabled || !Bukkit.isPrimaryThread()) {
-            executor.execute(listener, event);
-            return;
-        }
-        try (final Timing ignored = timings.startTiming()){
-            executor.execute(listener, event);
-        }
-    }
-}
